# [Amphiris talisman]
#===================================================
command /talisman [<text>] [<text>] [<text>]:
   aliases: tal
   trigger:
      if player is op:
         #======
         if arg-1 is "add":
            #====== Stop conditions
            if arg-2 is not set:
               send "&d&l[&6&l⌛&d&l] &c/talisman add <numéro du sort> (en tenant le talisman dans la main)" to player
               stop
            #======
            if player's tool is air:
               send "&d&l[&6&l⌛&d&l] &cPas de talisman en main !" to player
               stop
            #======
            else if {talismanleglist::*} contains name of player's tool:
               send "&d&l[&6&l⌛&d&l] &cTalisman invalide (déjà un talisman) !" to player
               stop
            #======
            else if {talismanlist::*} contains name of player's tool:
               send "&d&l[&6&l⌛&d&l] &cTalisman déjà ajouté !" to player
               stop
            #======
            else if {parcheminlist::*} contains name of player's tool:
               send "&d&l[&6&l⌛&d&l] &cC'est un parchemin !" to player
               stop
            #======
            else if {parchemincslist::*} contains name of player's tool:
               send "&d&l[&6&l⌛&d&l] &cC'est un parchemin CS !" to player
               stop
            #====== Effect
            add name of player's tool to {talismanlist::*}
            set {talisman::%name of player's tool%::1} to arg-2
            send "&d&l[&6&l⌛&d&l] %name of player's tool% &aajouté" to player
         #======
         else if arg-1 is "addleg":
            #====== Stop conditions
            if arg-2 is not set:
               send "&d&l[&6&l⌛&d&l] &c/talisman addleg <numéro du sort> (en tenant le talisman dans la main)" to player
               stop
            #======
            if player's tool is air:
               send "&d&l[&6&l⌛&d&l] &cPas de talisman légendaire en main !" to player
               stop
            #======
            else if {talismanleglist::*} contains name of player's tool:
               send "&d&l[&6&l⌛&d&l] &cTalisman légendaire déjà ajouté !" to player
               stop
            #======
            else if {talismanlist::*} contains name of player's tool:
               send "&d&l[&6&l⌛&d&l] &cTalisman invalide (déjà un talisman) !" to player
               stop
            #======
            else if {parcheminlist::*} contains name of player's tool:
               send "&d&l[&6&l⌛&d&l] &cC'est un parchemin !" to player
               stop
            #======
            else if {parchemincslist::*} contains name of player's tool:
               send "&d&l[&6&l⌛&d&l] &cC'est un parchemin CS !" to player
               stop
            #====== Effect
            add name of player's tool to {talismanleglist::*}
            set {talismanleg::%name of player's tool%::1} to arg-2
            send "&d&l[&6&l⌛&d&l] %name of player's tool% &aajouté" to player
         #======
         else if arg-1 is "addparch":
            #====== Stop conditions
            if arg-2 is not set:
               send "&d&l[&6&l⌛&d&l] &c/talisman addparch <numéro du sort> (en tenant le parchemin dans la main)" to player
               stop
            #======
            if player's tool is air:
               send "&d&l[&6&l⌛&d&l] &cPas de parchemin en main !" to player
               stop
            #======
            else if {talismanleglist::*} contains name of player's tool:
               send "&d&l[&6&l⌛&d&l] &cC'est un talisman légendaire !" to player
               stop
            #======
            else if {talismanlist::*} contains name of player's tool:
               send "&d&l[&6&l⌛&d&l] &cC'est un talisman !" to player
               stop
            #======
            else if {parcheminlist::*} contains name of player's tool:
               send "&d&l[&6&l⌛&d&l] &cParchemin déjà ajouté !" to player
               stop
            #======
            else if {parchemincslist::*} contains name of player's tool:
               send "&d&l[&6&l⌛&d&l] &cC'est un parchemin CS !" to player
               stop
            #====== Effect
            add name of player's tool to {parcheminlist::*}
            set {parchemin::%name of player's tool%::1} to arg-2
            send "&d&l[&6&l⌛&d&l] %name of player's tool% &aajouté" to player
         #======
         else if arg-1 is "addparchcs":
            #====== Stop conditions
            if arg-2 is not set:
               send "&d&l[&6&l⌛&d&l] &c/talisman addparchcs <numéro de l'église> (en tenant le parchemin dans la main)" to player
               stop
            #======
            if player's tool is air:
               send "&d&l[&6&l⌛&d&l] &cPas de parchemin en main !" to player
               stop
            #======
            else if {talismanleglist::*} contains name of player's tool:
               send "&d&l[&6&l⌛&d&l] &cC'est un talisman légendaire !" to player
               stop
            #======
            else if {talismanlist::*} contains name of player's tool:
               send "&d&l[&6&l⌛&d&l] &cC'est un talisman !" to player
               stop
            #======
            else if {parcheminlist::*} contains name of player's tool:
               send "&d&l[&6&l⌛&d&l] &cC'est un parchemin classique !" to player
               stop
            #======
            else if {parchemincslist::*} contains name of player's tool:
               send "&d&l[&6&l⌛&d&l] &cParchemin CS déjà ajouté !" to player
               stop
            #====== Effect
            add name of player's tool to {parchemincslist::*}
            set {parchemincs::%name of player's tool%::1} to arg-2
            send "&d&l[&6&l⌛&d&l] %name of player's tool% &aajouté" to player
         #======
         else if arg-1 is "addarme":
            #====== Stop conditions
            if arg-2 is not set:
               send "&d&l[&6&l⌛&d&l] &c/talisman addarme <nom du sort> <magie> (en tenant l'arme dans la main)" to player
               stop
            #======
            if player's tool is air:
               send "&d&l[&6&l⌛&d&l] &cPas d'arme en main !" to player
               stop
            #======
            else if {talismanleglist::*} contains name of player's tool:
               send "&d&l[&6&l⌛&d&l] &cC'est un talisman légendaire !" to player
               stop
            #======
            else if {talismanlist::*} contains name of player's tool:
               send "&d&l[&6&l⌛&d&l] &cC'est un talisman !" to player
               stop
            #======
            else if {parcheminlist::*} contains name of player's tool:
               send "&d&l[&6&l⌛&d&l] &cC'est un parchemin !" to player
               stop
            #======
            else if {armelist::*} contains name of player's tool:
               send "&d&l[&6&l⌛&d&l] &cArme déjà ajouté !" to player
               stop
            #======
            else if {parchemincslist::*} contains name of player's tool:
               send "&d&l[&6&l⌛&d&l] &cC'est un parchemin CS !" to player
               stop
            #====== Effect
            add name of player's tool to {armelist::*}
            set {arme::%name of player's tool%::1} to arg-2
            set {arme::%name of player's tool%::2} to arg-3
            send "&d&l[&6&l⌛&d&l] %name of player's tool% &aajouté" to player
         #======
         else if arg-1 is "del":
            #====== Stop conditions
            if player's tool is air:
               send "&d&l[&6&l⌛&d&l] &cPas de talisman en main !" to player
               stop
            #======
            else if {talismanlist::*} doesn't contain name of player's tool:
               send "&d&l[&6&l⌛&d&l] &cPas de talisman en main !" to player
               stop
            #====== Effect
            delete {talisman::%name of player's tool%::1}
            remove name of player's tool from {talismanlist::*}
            send "&d&l[&6&l⌛&d&l] %name of player's tool% &csupprimé" to player
         #======
         else if arg-1 is "delleg":
            #====== Stop conditions
            if player's tool is air:
               send "&d&l[&6&l⌛&d&l] &cPas de talisman légendaire en main !" to player
               stop
            #======
            else if {talismanleglist::*} doesn't contain name of player's tool:
               send "&d&l[&6&l⌛&d&l] &cPas de talisman légendaire en main !" to player
               stop
            #====== Effect
            delete {talismanleg::%name of player's tool%::1}
            remove name of player's tool from {talismanleglist::*}
            send "&d&l[&6&l⌛&d&l] %name of player's tool% &csupprimé" to player
         #======
         else if arg-1 is "delparch":
            #====== Stop conditions
            if player's tool is air:
               send "&d&l[&6&l⌛&d&l] &cPas de parchemin en main !" to player
               stop
            #======
            else if {parcheminlist::*} doesn't contain name of player's tool:
               send "&d&l[&6&l⌛&d&l] &cPas de parchemin en main !" to player
               stop
            #====== Effect
            delete {parchemin::%name of player's tool%::1}
            remove name of player's tool from {parcheminlist::*}
            send "&d&l[&6&l⌛&d&l] %name of player's tool% &csupprimé" to player
         #======
         else if arg-1 is "delparchcs":
            #====== Stop conditions
            if player's tool is air:
               send "&d&l[&6&l⌛&d&l] &cPas de parchemin CS en main !" to player
               stop
            #======
            else if {parchemincslist::*} doesn't contain name of player's tool:
               send "&d&l[&6&l⌛&d&l] &cPas de parchemin CS en main !" to player
               stop
            #====== Effect
            delete {parchemincs::%name of player's tool%::1}
            remove name of player's tool from {parchemincslist::*}
            send "&d&l[&6&l⌛&d&l] %name of player's tool% &csupprimé" to player
         #======
         else if arg-1 is "delarme":
            #====== Stop conditions
            if player's tool is air:
               send "&d&l[&6&l⌛&d&l] &cPas d'arme en main !" to player
               stop
            #======
            else if {armelist::*} doesn't contain name of player's tool:
               send "&d&l[&6&l⌛&d&l] &cPas d'arme en main !" to player
               stop
            #====== Effect
            delete {arme::%name of player's tool%::2}
            delete {arme::%name of player's tool%::1}
            remove name of player's tool from {armelist::*}
            send "&d&l[&6&l⌛&d&l] %name of player's tool% &csupprimé" to player
         #======
         else if arg-1 is "delall":
            #delete {talisman::*}
            #delete {talismanlist::*}
            #delete {talismanleg::*}
            #delete {talismanleglist::*}
            #send "&d&l[&6&l⌛&d&l] &cTous les talismans ont été supprimés !" to player
            send "&d&l[&6&l⌛&d&l] &cCommande désactivée" to player
         #======
         else if arg-1 is "list":
            send "&d&l[&6&l⌛&d&l] &fListe des talismans:" to player
            loop {talismanlist::*}:
               wait 0.05 second
               send "- %loop-value%" to player

            send "" to player
            send "&d&l[&6&l⌛&d&l] &fListe des talismans légendaires:" to player
            loop {talismanleglist::*}:
               wait 0.05 second
               send "- %loop-value%" to player

            send "" to player
            send "&d&l[&6&l⌛&d&l] &fListe des parchemins:" to player
            loop {parcheminlist::*}:
               wait 0.05 second
               send "- %loop-value%" to player

            send "" to player
            send "&d&l[&6&l⌛&d&l] &fListe des parchemins CS:" to player
            loop {parchemincslist::*}:
               wait 0.05 second
               send "- %loop-value%" to player

            send "" to player
            send "&d&l[&6&l⌛&d&l] &fListe des armes:" to player
            loop {armelist::*}:
               wait 0.05 second
               send "- %loop-value%" to player
         #======
         else if arg-1 is "help":
            send "&d&l[&6&l⌛&d&l] &eManuel du système de talisman:" to player
            send "&e- /tal add <numéro du sort> &7- Ajoute l'item en main dans la liste des talismans et l'associe au sort indiqué" to player
            send "&e- /tal addleg <numéro du sort> &7- Ajoute l'item en main dans la liste des talismans légendaires et l'associe au sort indiqué" to player
            send "&e- /tal addparch <numéro du sort> &7- Ajoute l'item en main dans la liste des parchemins et l'associe au sort indiqué" to player
            send "&e- /tal addparchcs <numéro de l'église> &7- Ajoute l'item en main dans la liste des parchemins CS et l'associe au CS indiqué" to player
            send "&e- /tal addarme <nom du sort> <magie> &7- Ajoute l'item en main dans la liste des armes et l'associe à l'effet indiqué" to player
            send "&e- /tal del &7- Retire l'item en main de la liste des talismans" to player
            send "&e- /tal delleg &7- Retire l'item en main de la liste des talismans légendaires" to player
            send "&e- /tal delparch &7- Retire l'item en main de la liste des parchemins" to player
            send "&e- /tal delparchcs &7- Retire l'item en main de la liste des parchemins CS" to player
            send "&e- /tal delarme &7- Retire l'item en main de la liste des armes" to player
            send "&e- /tal delall &7- Retire tous les talismans (talismans légendaires inclus)" to player
            send "&e- /tal list &7- Affiche la liste des objets cliquables" to player
         #======
         else:
            send "&d&l[&6&l⌛&d&l] &cTapez /tal help pour voir comment marche la commande" to player
         #======
      else:
         send "&d&l[&6&l⌛&d&l] &4Vous n'avez pas accès à cette commande." to player
#===================================================
on left click:
   if {talismanlist::*} contains name of player's tool:
      cancel event
   else if {talismanleglist::*} contains name of player's tool:
      cancel event
   else if {parcheminlist::*} contains name of player's tool:
      cancel event
#======
on right click:
   if {talismanlist::*} contains name of player's tool:
      cancel event
      make console execute command "/execute as %player% at @s run scoreboard players set @s[tag=unlock%{talisman::%name of player's tool%::1}%] molette %{talisman::%name of player's tool%::1}%"
      #======
      if {talisman::%name of player's tool%::1} is "1":
