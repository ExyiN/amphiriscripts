         send "&cSortilège sélectionné: &eUn fragment d'Éternité" to player
      else if {talismanleg::%name of player's tool%::1} is "117":
         make console execute command "/execute as %player% at @s run scoreboard players set @s[tag=unlockexo1] molette %{talismanleg::%name of player's tool%::1}%"
         send "&cSortilège sélectionné: &eLumière salutaire" to player
      else if {talismanleg::%name of player's tool%::1} is "118":
         make console execute command "/execute as %player% at @s run scoreboard players set @s[tag=unlockexo2] molette %{talismanleg::%name of player's tool%::1}%"
         send "&cSortilège sélectionné: &eExorcisme" to player
      #======
   else if {parcheminlist::*} contains name of event-item:
      cancel event
      #======
      if {parchemin::%name of event-item%::1} is "1":
         set {_magie} to 1000
         set {_nomParch} to "neant"
