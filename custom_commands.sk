#Give debug stick
command /db [<player>]:
   trigger:
      if player is op:
         if arg-1 is not set:
            give debug stick to player
            stop
         give debug stick to arg-1
      else:
         send "&d&l[&6&l⌛&d&l] &4Vous n'avez pas accès à cette commande." to player
