#Cadre invisible et tag presen
command /cadre:
   trigger:
      if player is op:
         make player execute command "/data merge entity @e[type=item_frame,distance=..2,sort=nearest,limit=1] {Invisible:1b,Item:{}}"
         make player execute command "/tag @e[type=item_frame,distance=..2,sort=nearest,limit=1] add presen"
         make player execute command "/data merge entity @e[type=glow_item_frame,distance=..2,sort=nearest,limit=1] {Invisible:1b,Item:{}}"
         make player execute command "/tag @e[type=glow_item_frame,distance=..2,sort=nearest,limit=1] add presen"
      else:
         send "&d&l[&6&l⌛&d&l] &4Vous n'avez pas accès à cette commande." to player
